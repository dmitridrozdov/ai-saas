{
    "rewrites": [
      {
        "source": "/app/dashboard/routes/priceradar/:path*",
        "destination": "/app/dashboard/routes/priceradar/:path*"
      }
    ],
    "headers": [
      {
        "source": "/app/dashboard/routes/priceradar/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,PUT,PATCH,POST,DELETE" },
          { "key": "Access-Control-Allow-Headers", "value": "*" }
        ]
      }
    ]
  }
  